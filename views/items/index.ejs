<!DOCTYPE html>
<html lang="en">
    <head>
        <%- include('./partials/head', { name, description, image, quality }); %>
    </head>

    <body class="container">
        <header><%- include('./partials/header'); %></header>
        <main>
            <div class="jumbotron">
                <div class="column">
                    <div class="row">
                        <div class="quality-box"></div>
                        <div class="row" id="item-name">
                            <h1><%= name %></h1>
                            <input id="input-name" , type="text" , value="<%= name %>" /><button
                                class="copy"
                                id="copyName"
                            >
                                <img
                                    src="data:image/svg+xml;base64,"
                                    alt=""
                                />
                            </button>
                        </div>
                    </div>
                    <div class="row" id="item-sku">
                        <h3><%= skuForDisplay %></h3>
                        <input id="input-sku" , type="text" , value="<%= skuForDisplay %>" /><button
                            class="copy"
                            id="copySku"
                        >
                            <img
                                src="data:image/svg+xml;base64,"
                                alt=""
                            />
                        </button>
                    </div>
                </div>

                <div class="jumbotron-content" align="center">
                    <div>
                        <img src="<%= image %>" alt="<%= name %>" width="500" height="500" />
                    </div>
                    <div>
                        <a id="bptfLink" href="<%= bptfUrl %>">Backpack.tf (Next)</a>
                        <a id="oldBptfLink" href="<%= oldBptfUrl %>">Backpack.tf</a>
                    </div>
                    <div>
                        <a id="mptfLink" href="https://marketplace.tf/items/tf2/<%= sku %>">Marketplace.tf</a>
                        <a id="pricestfLink" href="https://prices.tf/items/<%= sku %>"
                            ><%= currentPricestfPrice ? currentPricestfPrice : 'Prices.tf' %></a
                        >
                    </div>
                    <div>
                        <a id="scmLink" href="<%= scmUrl %>">Steam Community Market</a>
                        <a id="stnLink" href="<%= stnUrl %>">STN Trading</a>
                    </div>
                    <div>
                        <a id="gladiatorSales" href="https://gladiator.tf/sales?item=<%= name %>"
                            >Gladiator.tf (Sales)</a
                        >
                        <% if (isCrate) { %>
                            <a id="cratetfLink" href="https://crate.tf/item/<%= sku %>">Crate.tf</a>
                        <% } %>
                    </div>
                </div>
            </div>

            <script>
                const elem = document.querySelector('.quality-box');
                elem.style.backgroundColor = '<%= quality %>';
            </script>

            <script
                type="text/javascript"
                src="/js/getMptfPrice.js"
                id="getMptfPrice"
                bptfQuery="<%= bptfQuery %>"
            ></script>

            <script>
                document.querySelector('#copyName').addEventListener('click', function () {
                    let input = document.getElementById('input-name');
                    input.focus();
                    input.select();
                    document.execCommand('copy');
                });
                document.querySelector('#copySku').addEventListener('click', function () {
                    let input = document.getElementById('input-sku');
                    input.focus();
                    input.select();
                    document.execCommand('copy');
                });
            </script>
        </main>
        <footer><%- include('./partials/footer'); %></footer>
    </body>
</html>
